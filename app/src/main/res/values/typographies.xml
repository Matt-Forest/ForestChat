<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Typography" />

    <style name="Typography.H6">
        <item name="fontFamily">@font/mulish_semibold</item>
        <item name="android:textSize">20sp</item>
        <item name="lineHeight">25.1sp</item>
        <item name="android:letterSpacing">0.15</item>
    </style>

    <style name="Typography.Subtitle1">
        <item name="fontFamily">@font/mulish_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="lineHeight">20.1sp</item>
        <item name="android:letterSpacing">0.15</item>
    </style>

    <style name="Typography.Subtitle2">
        <item name="fontFamily">@font/mulish_bold</item>
        <item name="android:textSize">14sp</item>
        <item name="lineHeight">17.6sp</item>
        <item name="android:letterSpacing">0.15</item>
    </style>

    <style name="Typography.Caption">
        <item name="fontFamily">@font/mulish_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="lineHeight">15.1sp</item>
        <item name="android:letterSpacing">0.4</item>
    </style>

    <style name="Typography.Button">
        <item name="fontFamily">@font/mulish_semibold</item>
        <item name="android:textSize">14sp</item>
        <item name="lineHeight">17.6sp</item>
        <item name="android:letterSpacing">1.25</item>
    </style>

    <style name="Typography.Overline">
        <item name="fontFamily">@font/mulish_bold</item>
        <item name="android:textSize">10sp</item>
        <item name="lineHeight">12.6sp</item>
        <item name="android:letterSpacing">1.5</item>
    </style>

</resources>